<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Email</th>
      <th scope="col">Customize</th>
    </tr>
  </thead>
  <tbody>
    {{#each rows}} <!-- iterates the data from the object rows -->
    <tr>
      <th scope="row">{{this.usersId}}</th>
      <td>{{this.usersFN}}</td>
      <td>{{this.usersLN}}</td>
      <td>{{this.usersEmail}}</td>
      <td>
        <a type="button" class="btn btn-dark btn-sm mt-1 mb-1"><i class="bi bi-eye"></i> View</a>
        <a type="button" class="btn btn-dark btn-sm mt-1 mb-1"><i class="bi bi-trash"></i> Edit</a>
        <a type="button" class="btn btn-dark btn-sm mt-1 mb-1"><i class="bi bi-eye"></i> Delete</a>
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>